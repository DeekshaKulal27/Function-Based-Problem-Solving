    "def check_medicine_stock(stock):\n",
    "    \n",
    "    print(\"Medicine Stock:\", stock)\n",
    "    \n",
    "    # Conditional logic\n",
    "    if stock < 10:\n",
    "        print(\"Status: Low Stock Alert\")\n",
    "    else:\n",
    "        print(\"Status: Stock Sufficient\")\n",
    "\n",
    "\n",
    "# Example Usage\n",
    "check_medicine_stock(6)"

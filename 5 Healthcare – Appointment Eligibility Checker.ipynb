    "def check_appointment_eligibility(age):\n",
    "    \n",
    "    print(\"Patient Age:\", age)\n",
    "    \n",
    "    # Condition check\n",
    "    if age >= 18:\n",
    "        print(\"Eligibility Status: Eligible\")\n",
    "    else:\n",
    "        print(\"Eligibility Status: Not Eligible\")\n",
    "\n",
    "\n",
    "# Example Usage\n",
    "check_appointment_eligibility(21)"
